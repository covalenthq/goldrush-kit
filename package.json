{
    "name": "@covalenthq/goldrush-kit",
    "version": "0.5.2",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/goldrush-kit.umd.cjs",
            "import": "./dist/goldrush-kit.es.js",
            "types": "./dist/index.d.ts"
        },
        "./styles.css": "./dist/styles.css"
    },
    "type": "module",
    "main": "dist/goldrush-kit.umd.cjs",
    "module": "dist/goldrush-kit.es.js",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "/dist"
    ],
    "repository": "https://github.com/covalenthq/goldrush-kit",
    "author": "Covalent",
    "license": "Apache-2.0",
    "scripts": {
        "build:library": "vite build && npm run build:library-css",
        "build:library-css": "tailwindcss -m -i ./src/tailwind-input.css -o ./dist/styles.css --minify",
        "build:storybook": "storybook build && npm run build:storybook-css",
        "build:storybook-css": "tailwindcss -m -i ./src/tailwind-input.css -o ./src/tailwind-output.css --minify",
        "bundle-size": "npx vite-bundle-visualizer",
        "dev": "concurrently -n \"storybook-css,storybook\" -c \"blue,red\" \"npm run dev:css\" \"storybook dev -p 6006\"",
        "dev:css": "tailwindcss -w -i ./src/tailwind-input.css -o ./src/tailwind-output.css --minify",
        "lint": "eslint ./src/",
        "prepublishOnly": "npm run build:library",
        "pretty": "prettier . --write"
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@storybook/addon-essentials": "^7.5.3",
        "@storybook/addon-interactions": "^7.5.3",
        "@storybook/addon-links": "^7.5.3",
        "@storybook/addon-styling": "^1.3.7",
        "@storybook/blocks": "^7.5.3",
        "@storybook/react": "^7.5.3",
        "@storybook/react-vite": "^7.5.3",
        "@storybook/testing-library": "0.2.2",
        "@tanstack/react-table": "^8.10.7",
        "@tremor/react": "^3.14.0",
        "@types/lodash": "^4.14.200",
        "@types/react": "^18.2.29",
        "@types/react-dom": "^18.2.14",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@vitejs/plugin-react": "^3.1.0",
        "autoprefixer": "^10.4.17",
        "babel-loader": "^8.3.0",
        "chromatic": "^7.4.0",
        "concurrently": "^7.6.0",
        "eslint": "^8.51.0",
        "eslint-config-standard-with-typescript": "^39.1.1",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-n": "^16.2.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-storybook": "^0.6.15",
        "eslint-plugin-tailwindcss": "^3.13.0",
        "postcss": "^8.4.35",
        "prettier": "^3.1.0",
        "prettier-plugin-tailwindcss": "^0.5.7",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "storybook": "^7.5.3",
        "storybook-addon-themes": "^6.1.0",
        "tailwindcss": "^3.4.1",
        "typescript": "^4.9.5",
        "vite": "^4.5.2",
        "vite-plugin-dts": "^1.7.3",
        "vite-plugin-lib-inject-css": "^1.3.0",
        "vite-tsconfig-paths": "^4.2.1"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "dependencies": {
        "@covalenthq/client-sdk": "^1.0.0",
        "@headlessui/react": "^1.7.17",
        "@radix-ui/react-avatar": "^1.0.4",
        "@radix-ui/react-checkbox": "^1.0.4",
        "@radix-ui/react-dialog": "^1.0.5",
        "@radix-ui/react-dropdown-menu": "^2.0.6",
        "@radix-ui/react-icons": "^1.3.0",
        "@radix-ui/react-popover": "^1.0.7",
        "@radix-ui/react-separator": "^1.0.3",
        "@radix-ui/react-slot": "^1.0.2",
        "@radix-ui/react-toast": "^1.1.5",
        "@storybook/theming": "^7.5.3",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "cmdk": "^0.2.1",
        "dayjs": "^1.11.10",
        "react-qr-code": "^2.0.12",
        "storybook-dark-mode": "^3.0.1",
        "storybook-tailwind-dark-mode": "^1.0.22",
        "tailwind-merge": "^1.14.0",
        "tailwindcss-animate": "^1.0.7"
    }
}
